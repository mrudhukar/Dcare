<h2>Listing Medical Reports</h2>
 
<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Diagnosis Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
<% @diagnoses.each do |diagnosis| %>
  <tr>
    <td><%= diagnosis.name %></td>
    <td><%= diagnosis.description %></td>
    <td><%= diagnosis.diagnosis_date %></td>
    <td><%= link_to 'Show', diagnosis %></td>
    <td><%= link_to 'Edit', edit_diagnosis_path(diagnosis) %></td>
    <td><%= link_to 'Delete', diagnosis, :confirm => 'Are you sure?',
                                     :method => :delete %></td>
  </tr>
<% end %>
</table>
 
<br />
 
<%= link_to 'New Report', new_diagnosis_path %>